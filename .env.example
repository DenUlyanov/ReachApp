# LinkedIn Bot Environment Configuration
# Copy this file to .env and fill in your credentials

# ============================================================================
# LinkedIn Credentials (REQUIRED for login functionality)
# ============================================================================
LINKEDIN_EMAIL=your-email@example.com
LINKEDIN_PASSWORD=your-password-here

# ============================================================================
# Sales Navigator Configuration
# ============================================================================
SALES_NAVIGATOR_URL=https://www.linkedin.com/sales

# ============================================================================
# Bot Configuration
# ============================================================================

# Browser mode (true = no GUI, false = show browser)
HEADLESS_MODE=false

# Slow motion delay in milliseconds (for debugging, 0 = off)
SLOW_MO=0

# Default timeout in milliseconds
TIMEOUT=30000

# Maximum login retry attempts
MAX_LOGIN_ATTEMPTS=3

# ============================================================================
# Logging Configuration
# ============================================================================

# Log level: DEBUG, INFO, WARNING, ERROR, CRITICAL
LOG_LEVEL=INFO

# ============================================================================
# Testing Configuration (Optional)
# ============================================================================

# Enable testing mode (disables credential warnings)
# TESTING=false
